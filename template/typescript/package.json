{
  "name": "pocketflow-template-typescript",
  "version": "0.1.0",
  "description": "TypeScript template for PocketFlow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "ts-node src/index.ts",
    "start": "npm run build && node dist/index.js",
    "demo": "ts-node src/examples/simple-example.ts",
    "test": "vitest",
    "test:unit": "vitest test/unit",
    "test:integration": "vitest test/integration",
    "test:e2e": "vitest test/e2e",
    "test:performance": "vitest test/performance",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.js,.jsx,.tsx"
  },
  "keywords": [
    "pocketflow",
    "template",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^18.16.0",
    "@types/prompt-sync": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "prettier": "^2.8.8",
    "prompt-sync": "^4.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "openai": "^4.90.0",
    "pocketflow": "^1.0.4"
  }
}
